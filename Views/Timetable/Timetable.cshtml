
@{
    ViewBag.Title = "TimetableView";
}

<h2>Ваше расписание на эту неделю</h2>

<div>
    <table>
        @foreach (var day in ViewBag.Week)
        {
            <tr>
                <td></td><td>@("\t")</td> <td>@day.name</td>
            </tr>
            
            foreach(var lesson in day.lessons)
            {
    <tr>
        <td>@(lesson.time + "\t")</td>
        <td>@("\t")</td>
        <td>@(lesson.name + "\t")</td>
    </tr>
            }
            <br>
        }
    </table>
</div>